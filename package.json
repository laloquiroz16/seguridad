    "node": "10.23.0",
    "npm": "6.x.x"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "npm run webpack-dev-server -- --env.environment=prod",
    "start:dev": "npm run webpack-dev-server -- --env.environment=dev",
    "start:server": "nodemon ./server.js",
    "start:ssl": "npm run webpack-dev-server -- --https --key ./ssl/server.key --cert ./ssl/server.crt --env.environment=dev",
    "prebuild": "rimraf dist",
    "prebuild:drone": "rimraf dist",
    "prebuild:production": "npm run translations:upload",
    "build": "node --max_old_space_size=8192 ./node_modules/.bin/webpack",
    "build:production": "npm run build -- --env.environment=prod",
    "build:production:heroku": "node ./node_modules/.bin/webpack --env.environment=prod",
    "build:development": "npm run build -- --env.environment=dev",
    "analyze:dev": "npm run build -- --env.analyzer=true",
    "analyze:prod": "npm run build:production -- --env.analyzer=true",
    "e2e": "protractor protractor.conf.js",
    "i18n": "i18n-generate -s app/i18n/es -o app/i18n -l 'es pt en fr de pl' -d es -i common",
    "i18n:aws": "aws-i18n-translations -s app/i18n/es -o app/i18n -l 'es pt en fr de pl' -d es -i common",
    "heroku-postbuild": "npm run translations:upload && npm run build:production:heroku",
    "webpack-dev-server": "node --max_old_space_size=8192 ./node_modules/.bin/webpack-dev-server --color --hot --open",
    "pretranslations:upload": "npm run i18n",
    "translations:upload": "node ./app/i18n/post-translations.js",
    "test": "node --max_old_space_size=8192 --expose-gc ./node_modules/.bin/jest -u",
    "test:watch": "npm t -- --watch",
    "test:mem": "npm t -- --logHeapUsage",
    "lint": "npm run lint:app && npm run lint:src",
    "lint:fix": "npm run lint -- --fix",
    "lint:app": "eslint \"app/modules/**/*.{js,jsx,ts,tsx}\" --cache --cache-location \"node_modules/.cache/.eslintapp\"",
    "lint:src": "eslint \"src/**/*.{ts,tsx}\" --cache --cache-location \"node_modules/.cache/.eslintsrc\"",
    "format": "prettier \"app/**/*.{js,jsx,ts,tsx,css,less,scss,json,html,md}\"",
    "format:check": "npm run format -- --check",
    "format:write": "npm run format -- --write",
    "type:check": "tsc"
  },
  "husky": {
    "hooks": {}
  },
